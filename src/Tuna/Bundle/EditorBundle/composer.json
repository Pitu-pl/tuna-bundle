{
  "name": "tuna-cms/editor-bundle",
  "type": "symfony-bundle",
  "description": "Editor for Tuna CMS",
  "homepage": "https://thecodeine.com",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "symfony/symfony": "^2.8.0"
  },
  "scripts": {
    "post-install-cmd": [
      "npm install bower --save",
      "bower install"
    ],
    "post-update-cmd": [
      "npm update",
      "bower update"
    ]
  },
  "autoload": {
    "psr-4": {
      "TheCodeine\\EditorBundle\\": ""
    }
  }
}